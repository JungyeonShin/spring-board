<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybats//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jy.todo.mapper.MainMapper">
    <select id="selectTestValue" resultType="HashMap">
        SELECT ID
              ,TITLE
              ,CONTENT
              ,REG_MBR_ID
              ,REG_DT
              ,MODF_MBR_ID
              ,MODF_DT
          FROM TEST_BOARD
         WHERE ID = 1
    </select>
</mapper>